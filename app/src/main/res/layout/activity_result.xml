<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:padding="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="STUDENT APPLICATION FORM SUBMITTED"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/lastName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:text="" />
        <TextView
            android:id="@+id/firstName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/lastName"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/middleInitial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/firstName"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/gender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/middleInitial"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/contactNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/gender"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/apartmentName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/contactNumber"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/houseNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/apartmentName"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/lotNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/houseNumber"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/blockNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/lotNumber"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/street"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/blockNumber"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/barangay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/street"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/cityMunicipality"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/barangay"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/provinceState"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/cityMunicipality"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/provinceState"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/dialectKnown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/email"
            android:layout_marginTop="8dp"
            android:text="" />

        <TextView
            android:id="@+id/interviewTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/dialectKnown"
            android:layout_marginTop="8dp"
            android:text="" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>